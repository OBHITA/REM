  <DataSets>
    <DataSet Name="ActivitiesPerformedByPatientKey">
      <Query>
        <DataSourceName>RemDataSource</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@patientKey">
            <Value>=Parameters!patientKey.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT        at.Name AS Activity, v.Name AS Visit, p.FirstName + '  ' + p.LastName AS Patient, v.CheckedInDateTime
FROM            VisitModule.Activity AS a INNER JOIN
                         VisitModule.ActivityTypeLkp AS at ON at.ActivityTypeLkpKey = a.ActivityTypeLkpKey INNER JOIN
                         VisitModule.Visit AS v ON v.AppointmentKey = a.VisitKey INNER JOIN
                         ClinicalCaseModule.ClinicalCase AS c ON c.ClinicalCaseKey = v.ClinicalCaseKey INNER JOIN
                         PatientModule.Patient AS p ON p.PatientKey = @patientKey</CommandText>
      </Query>
      <Fields>
        <Field Name="Activity">
          <DataField>Activity</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Visit">
          <DataField>Visit</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Patient">
          <DataField>Patient</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="CheckedInDateTime">
          <DataField>CheckedInDateTime</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="PatientList">
      <Query>
        <DataSourceName>RemDataSource</DataSourceName>
        <CommandText>SELECT        PatientKey, FirstName + ' ' + LastName AS FullName
FROM            PatientModule.Patient</CommandText>
      </Query>
      <Fields>
        <Field Name="PatientKey">
          <DataField>PatientKey</DataField>
          <rd:TypeName>System.Int64</rd:TypeName>
        </Field>
        <Field Name="FullName">
          <DataField>FullName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
  <ReportParameters>
    <ReportParameter Name="patientKey">
      <DataType>String</DataType>
      <Prompt>Patient Name</Prompt>
      <ValidValues>
