                        <CustomControls:SearchBox.AdvancedContent>
                            <Border Width="auto" Height="Auto"  Style="{StaticResource TileSubHeaderBackgroundBorderStyle}" BorderThickness="0 1">
                                <Grid x:Name="AdvancedSearchFacilityGrid" MinWidth="800" Margin="0 0 0 10">
                                    <Grid.RowDefinitions>
                                        <RowDefinition/>
                                        <RowDefinition/>
                                    </Grid.RowDefinitions>
                                    <Border  Grid.Row="0" BorderBrush="DarkGray" BorderThickness="0 0 0 1" HorizontalAlignment="Stretch" Margin="40 10">
                                        <TextBlock Text="Advanced Search" Margin="10 0 0 0" FontSize="14"/>
                                    </Border>

                                    <Grid HorizontalAlignment="Center" Grid.Row="1" VerticalAlignment="Center" Margin="0 0 10 0">
                                        <Grid.RowDefinitions>
                                            <RowDefinition/>
                                            <RowDefinition/>
                                        </Grid.RowDefinitions>
                                        <StackPanel Grid.Row="0">

                                            <StackPanel.Resources>
                                                <Style TargetType="Controls:Label" BasedOn="{StaticResource EditFormLabelRemStyle}">
                                                    <Setter Property="Foreground" Value="Black"/>
                                                </Style>

                                                <Style TargetType="telerik:RadMaskedTextBox">
                                                    <Setter Property="MaskType" Value="None"/>
                                                    <Setter Property="EmptyContentTemplate" Value="{StaticResource WatermarkTemplate}"/>
                                                </Style>
                                            </StackPanel.Resources>

                                            <StackPanel Orientation="Horizontal" Margin="0,5" Grid.Row="0" Grid.Column="0">
                                                <StackPanel Style="{StaticResource EditFormStackPanelStyle}">
                                                    <Controls:Label Content="First Name" Target="{Binding ElementName=FirstNameTextBox}"/>
                                                    <telerik:RadMaskedTextBox x:Name="FirstNameTextBox" EmptyContent="First Name" MaskedText="{Binding AdvancedSearchCriteria.FirstNameToSearch, Mode=TwoWay}" />
                                                </StackPanel>
                                                <StackPanel Style="{StaticResource EditFormStackPanelStyle}">
                                                    <Controls:Label Content="Middle Name" Target="{Binding ElementName=MiddleNameTextBox}"/>
                                                    <telerik:RadMaskedTextBox x:Name="MiddleNameTextBox" EmptyContent="Middle Name" MaskedText="{Binding AdvancedSearchCriteria.MiddleNameToSearch, Mode=TwoWay}"/>
                                                </StackPanel>

                                                <StackPanel Style="{StaticResource EditFormStackPanelStyle}">
                                                    <Controls:Label Content="Last Name" Target="{Binding ElementName=LastNameTextBox}"/>
                                                    <telerik:RadMaskedTextBox x:Name="LastNameTextBox" EmptyContent="Last Name" MaskedText="{Binding AdvancedSearchCriteria.LastNameToSearch, Mode=TwoWay}"/>
                                                </StackPanel>

                                                <StackPanel Style="{StaticResource EditFormStackPanelStyleXS}">
                                                    <Controls:Label Content="Suffix" Target="{Binding ElementName=SuffixComboBox}"/>
                                                    <telerik:RadComboBox x:Name="SuffixComboBox" ItemsSource="{Binding LookupValueLists[Suffix], Mode=OneWay}" SelectedItem="{Binding AdvancedSearchCriteria.SuffixToSearch, Mode=TwoWay}" DisplayMemberPath="Name"/>
                                                </StackPanel>
                                            </StackPanel>

                                            <StackPanel Orientation="Horizontal">
                                                <StackPanel Style="{StaticResource EditFormStackPanelStyle}">
                                                    <Controls:Label Content="Gender" Target="{Binding ElementName=GenderComboBox}"/>
                                                    <telerik:RadComboBox x:Name="GenderComboBox" EmptyText="Select Gender" ItemsSource="{Binding LookupValueLists[PatientGender], Mode=OneWay}" SelectedItem="{Binding AdvancedSearchCriteria.GenderToSearch, Mode=TwoWay}" DisplayMemberPath="Name"/>
                                                </StackPanel>
                                                <StackPanel Style="{StaticResource EditFormStackPanelStyleS}">
                                                    <Controls:Label Content="Birth Date" Target="{Binding ElementName=BirthDatePicker}"/>
                                                    <telerik:RadDateTimePicker x:Name="BirthDatePicker" InputMode="DatePicker" SelectedValue="{Binding AdvancedSearchCriteria.BirthDateToSearch, Mode=TwoWay}"/>
                                                </StackPanel>
                                                <StackPanel  Style="{StaticResource EditFormStackPanelStyle}">
                                                    <Controls:Label Content="Mother's Maiden Name" Target="{Binding ElementName=MotherMaidenNameTextBox}"/>
                                                    <telerik:RadMaskedTextBox x:Name="MotherMaidenNameTextBox" MaskedText="{Binding AdvancedSearchCriteria.MotherMaidenNameToSearch, Mode=TwoWay}"  EmptyContent="Mother's Maiden Name" />
                                                </StackPanel>
                                            </StackPanel>


                                            <Border Style="{StaticResource HorizontalLineEditFormDivider}"/>

                                            <StackPanel Orientation="Horizontal">
                                                <StackPanel  Style="{StaticResource EditFormStackPanelStyle}">
                                                    <Controls:Label Content="Patient ID Number" Target="{Binding ElementName=IdTextBox}"/>
                                                    <telerik:RadMaskedTextBox x:Name="IdTextBox" MaskedText="{Binding AdvancedSearchCriteria.UniqueIdentifierToSearch, Mode=TwoWay}" EmptyContent="Patient ID #" />
                                                </StackPanel>
                                                <Border BorderBrush="DarkGray" BorderThickness="1,0,0,0" Margin="29 0 0 0"/>
                                                <Border BorderBrush="White" BorderThickness="1,0,0,0" Margin="0 0 19 0"/>
                                                <StackPanel  Style="{StaticResource EditFormStackPanelStyle}">
                                                    <Controls:Label Content="Alternate ID Number" Target="{Binding ElementName=OtherNumberTypeComboBox}" />
                                                    <telerik:RadComboBox x:Name="IdentifierTypeComboBox" EmptyText="Select Identifier Type" ItemsSource="{Binding LookupValueLists[PatientIdentifierType], Mode=OneWay}" SelectedItem="{Binding AdvancedSearchCriteria.IdentifierTypeToSearch, Mode=TwoWay}" DisplayMemberPath="Name"/>
                                                </StackPanel>
                                                <StackPanel  Style="{StaticResource EditFormStackPanelStyle}" VerticalAlignment="Bottom">
                                                    <telerik:RadMaskedTextBox x:Name="IdentifierTextBox" MaskedText="{Binding AdvancedSearchCriteria.IdentifierToSearch, Mode=TwoWay}" EmptyContent="Alternate ID #"  IsEnabled="{Binding IsIdentifierTypeToSearchNotNull}"/>
                                                </StackPanel>
                                            </StackPanel>

                                            <Border Style="{StaticResource HorizontalLineEditFormDivider}"/>

                                            <StackPanel Orientation="Horizontal">
                                                <StackPanel  Style="{StaticResource EditFormStackPanelStyle}">
                                                    <Controls:Label Content="Address" Target="{Binding ElementName=AddressLineOneTextBox}"/>
                                                    <telerik:RadMaskedTextBox x:Name="AddressLineOneTextBox" MaskedText="{Binding AdvancedSearchCriteria.AddressLineOneToSearch, Mode=TwoWay}" EmptyContent="Street Address" />
                                                </StackPanel>
                                                <StackPanel Orientation="Horizontal" VerticalAlignment="Bottom">
                                                    <StackPanel  Style="{StaticResource EditFormStackPanelStyle}">
                                                        <telerik:RadMaskedTextBox x:Name="CityTextBox" MaskedText="{Binding AdvancedSearchCriteria.CityToSearch, Mode=TwoWay}"  EmptyContent="City"/>
                                                    </StackPanel>
                                                    <StackPanel  Style="{StaticResource EditFormStackPanelStyleS}" Width="140">
                                                        <telerik:RadComboBox x:Name="StateComboBox" EmptyText="Select State" ItemsSource="{Binding LookupValueLists[StateProvince], Mode=OneWay}" SelectedItem="{Binding AdvancedSearchCriteria.StateToSearch, Mode=TwoWay}" DisplayMemberPath="Name"/>
                                                    </StackPanel>
                                                    <StackPanel  Style="{StaticResource EditFormStackPanelStyleS}" Width="80">
                                                        <telerik:RadMaskedTextBox x:Name="ZipCodeTextBox"  MaskedText="{Binding AdvancedSearchCriteria.ZipCodeToSearch, Mode=TwoWay}" EmptyContent="Zip Code"/>
                                                    </StackPanel>
                                                </StackPanel>
                                            </StackPanel>
                                        </StackPanel>
                                        <StackPanel Orientation="Horizontal" HorizontalAlignment="Right" Grid.Row="1" Margin="0 20 0 8">
                                            <telerik:RadButton x:Name="ResetButton" Style="{StaticResource StaticBorderlessButtonStyle}" Content="Clear" Command="{Binding ResetAdvancedSearchCommand}" Width="60" Height="22"/>
                                            <telerik:RadButton x:Name="SearchButton" Style="{StaticResource StaticBorderlessButtonStyle}" Content="Search" Command="{Binding RunAdvancedSearchCommand}" Width="60" Height="22"/>
                                        </StackPanel>
                                    </Grid>
                                </Grid>
                            </Border>
                        </CustomControls:SearchBox.AdvancedContent>

<!-- Add New Patient section-->
                        
                        <CustomControls:SearchBox.CreateNewContent>
                            <Border HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Style="{StaticResource TileSubHeaderBackgroundBorderStyle}" BorderThickness="0">
                                <Grid>
                                    <Grid.RowDefinitions>
                                        <RowDefinition/>
                                        <RowDefinition/>
                                    </Grid.RowDefinitions>
                                    <Border Grid.Row="0" BorderBrush="DarkGray" BorderThickness="0 0 0 1" HorizontalAlignment="Stretch" Margin="40 10">
                                        <TextBlock Text="New Patient" Margin="10 0" FontSize="14"/>
                                    </Border>
                                    <StackPanel HorizontalAlignment="Center" Grid.Row="1">
                                        <StackPanel Orientation="Horizontal" HorizontalAlignment="Center" Margin="0 0 10 20">
                                            <StackPanel.Resources>
                                                <Style TargetType="Controls:Label" BasedOn="{StaticResource EditFormLabelRemStyle}">
                                                    <Setter Property="Foreground" Value="Black"/>
                                                </Style>
                                                <Style TargetType="telerik:RadMaskedTextBox">
                                                    <Setter Property="MaskType" Value="None"/>
                                                    <Setter Property="EmptyContentTemplate" Value="{StaticResource WatermarkTemplate}"/>
                                                </Style>
                                            </StackPanel.Resources>
                                            <StackPanel Style="{StaticResource EditFormStackPanelStyle}">
                                                <Controls:Label Content="First Name" Target="{Binding ElementName=FirstNameTextBoxAddNew}"/>
                                                <telerik:RadMaskedTextBox x:Name="FirstNameTextBoxAddNew" MaskedText="{Binding AddNewPatient.FirstName, Mode=TwoWay}" EmptyContent="First Name"/>
                                            </StackPanel>
                                            <StackPanel Style="{StaticResource EditFormStackPanelStyle}">
                                                <Controls:Label Content="Last Name" Target="{Binding ElementName=LastNameTextBoxAddNew}"/>
                                                <telerik:RadMaskedTextBox x:Name="LastNameTextBoxAddNew" MaskedText="{Binding AddNewPatient.LastName, Mode=TwoWay}" EmptyContent="Last Name"/>
                                            </StackPanel>
                                            <StackPanel Style="{StaticResource EditFormStackPanelStyle}">
                                                <Controls:Label Content="Gender" Target="{Binding ElementName=GenderComboBoxNew}"/>
                                                <telerik:RadComboBox x:Name="GenderComboBoxNew" ItemsSource="{Binding LookupValueLists[PatientGender], Mode=OneWay}" SelectedItem="{Binding AddNewPatient.Gender, Mode=TwoWay}" DisplayMemberPath="Name"/>
                                            </StackPanel>
                                            <StackPanel Style="{StaticResource EditFormStackPanelStyleS}">
                                                <Controls:Label Content="Birth Date" Target="{Binding ElementName=BirthDatePickerNew}"/>
                                                <telerik:RadDateTimePicker x:Name="BirthDatePickerNew" InputMode="DatePicker" SelectedValue="{Binding AddNewPatient.BirthDate, Mode=TwoWay}"/>
                                            </StackPanel>
                                        </StackPanel>
                                        <StackPanel HorizontalAlignment="Right" Margin="0 0 10 10">
                                            <telerik:RadButton x:Name="SaveButton" Foreground="{StaticResource BorderlessButtonForeground}" ToolTipService.ToolTip="Save changes and add new patient." Style="{StaticResource StaticBorderlessButtonStyle}" Command="{Binding AddNewCommand}">
                                                <i:Interaction.Triggers>
                                                    <ei:DataTrigger Binding="{Binding AddNewState}" Comparison="Equal">
                                                        <ei:DataTrigger.Value>
                                                            <PatientSearch:AddNewState>Successful</PatientSearch:AddNewState>
                                                        </ei:DataTrigger.Value>
                                                        <i:InvokeCommandAction Command="{Binding Control.ClosePopupCommand, Source={StaticResource SearchControlProxy}}"/>
                                                    </ei:DataTrigger>
                                                </i:Interaction.Triggers>
                                                <ContentPresenter>
                                                    <Grid>
                                                        <Grid.ColumnDefinitions>
                                                            <ColumnDefinition Width="auto"/> 
                                                            <ColumnDefinition Width="*"/>
                                                        </Grid.ColumnDefinitions>
                                                        <Border Width="10" Height="10" Grid.Column="0">
                                                            <Path Stretch="Fill" Fill="{Binding Foreground, ElementName=SaveButton}" Data="F1 M 103.25,96.5L 105.25,96.5L 105.25,102.5L 111.25,102.5L 111.25,104.5L 105.25,104.5L 105.25,110.5L 103.25,110.5L 103.25,104.5L 97.25,104.5L 97.25,102.5L 103.25,102.5L 103.25,96.5 Z " VerticalAlignment="Center" HorizontalAlignment="Center"/>
                                                        </Border>
                                                        <TextBlock Grid.Column="1" VerticalAlignment="Center" Margin="6 0 0 0" Foreground="{Binding Foreground, ElementName=SaveButton}" Text="Add New Patient" FontFamily="{Binding FontFamily, ElementName=SaveButton}" FontSize="{Binding FontSize, ElementName=SaveButton}"/>
                                                    </Grid>
                                                </ContentPresenter>
                                            </telerik:RadButton>
                                        </StackPanel>
                                    </StackPanel>
                                </Grid>
                            </Border>
                        </CustomControls:SearchBox.CreateNewContent>
                    </CustomControls:SearchBox>
                </StackPanel>
            </telerik:RadTabItem>
        </telerik:RadTabControl>
    </Grid>
</UserControl>
