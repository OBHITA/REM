                                                    <StackPanel  Orientation="Horizontal">
                                                        <TextBlock Text="{Binding PhoneNumber, StringFormat='\{0\} '}" Foreground="{Binding Foreground}"/>
                                                        <TextBlock Text="{Binding PhoneExtensionNumber, StringFormat=(Ext.) \{0\}}" Foreground="{Binding Foreground}"/>
                                                    </StackPanel>
                                                </StackPanel>
                                            </DataTemplate>
                                        </ItemsControl.ItemTemplate>
                                    </ItemsControl>
