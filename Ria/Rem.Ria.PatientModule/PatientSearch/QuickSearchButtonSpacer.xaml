<UserControl x:Class="Rem.Ria.PatientModule.PatientSearch.QuickSearchButtonSpacer"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    d:DesignHeight="300" d:DesignWidth="400">

    <Grid x:Name="LayoutRoot">
        <Grid>
            <Rectangle StrokeThickness="0" StrokeLineJoin="Round" Width="1" Fill="#FF676767" HorizontalAlignment="Right" d:LayoutOverrides="Width"/>
            <Rectangle StrokeThickness="0" StrokeLineJoin="Round" Width="1" Fill="White" HorizontalAlignment="Left" d:LayoutOverrides="Width"/>
        </Grid>
    </Grid>
</UserControl>
