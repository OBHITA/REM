                                                <TextBlock Text="{Binding HispanicLatinoIndicator, Converter={StaticResource NullableBooleanToYesNoConverter}}" Style="{StaticResource EditFormDataElementTextBlockStyle}" Margin="5 0 0 0" FontWeight="Bold"/>
                                            </StackPanel>
                                            <CustomControls:NonResponseQuestionControl Question="G18. Do you have a Religious Preference?" 
                                                                           Hint="Ask, &quot;do you have a religious preference?&quot; This does not simply refer to  their childhood religion. Recommended Probes: Do you have any other spiritual belief system? Are you currently active/practicing this religion?"
                                                                           NonResponseItemsSource="{Binding Source={StaticResource RootDataContext},Path=DataSource.LookupValueLists[DensAsiNonResponse]}" 
                                                                           NonResponseFilters="{Binding DensAsiNonResponseFilters[PreferredDensAsiReligion]}" 
                                                                           DefaultNonResponseFilters="{Binding DefaultDensAsiNonResponseFilters}" 
                                                                           NonResponseDto="{Binding PreferredDensAsiReligion,Mode=TwoWay}" 
                                                                           ValueTemplate="{StaticResource ComboBoxLookupTemplate}"
                                                                           ReadOnlyValueTemplate="{StaticResource ReadOnlyLookupTemplate}"/>
                                            <CustomControls:AddRemoveCommentControl Text="{Binding PreferredDensAsiReligionNote, Mode=TwoWay}"/>

                                            <CustomControls:NonResponseQuestionControl Question="G19. Have you been in a controlled environment in past 30 days?" 
                                                                           Hint="A place, theoretically, without access to drugs/alcohol. If they have been in two controlled environments, record the one they have been in the longest. We recognize that clients may have access to alcohol and other drugs in these facilities."
                                                                           NonResponseItemsSource="{Binding Source={StaticResource RootDataContext},Path=DataSource.LookupValueLists[DensAsiNonResponse]}" 
                                                                           NonResponseFilters="{Binding DensAsiNonResponseFilters[LastThirtyDaysDensAsiControlledEnvironment]}" 
                                                                           DefaultNonResponseFilters="{Binding DefaultDensAsiNonResponseFilters}" 
