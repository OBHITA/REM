                                <CustomControls:EditableExpander.EditableContentTemplate>
                                    <DataTemplate>
                                        <CustomControls:EditableStackPanel Style="{StaticResource ScreenerSectionEditableStackPanelStyle}" IsReadOnly="True">
                                            <CustomControls:EditableStackPanel.Resources>
                                                <Style TargetType="CustomControls:NonResponseQuestionControl" BasedOn="{StaticResource DensAsiNonResponseQuestionStyle}">
                                                    <Setter Property="SharedGridLengthGroupName" Value="GeneralInformation"/>
                                                </Style>
                                                <Style TargetType="CustomControls:EditableGrid">
                                                    <Setter Property="Padding" Value="{StaticResource QuestionPadding}"/>
