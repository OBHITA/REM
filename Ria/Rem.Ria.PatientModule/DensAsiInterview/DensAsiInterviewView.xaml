                                                                           NonResponseFilters="{Binding DensAsiNonResponseFilters[LastThirtyDaysControlledEnvironmentDayCount]}" 
                                                                           DefaultNonResponseFilters="{Binding DefaultDensAsiNonResponseFilters}" 
                                                                           NonResponseDto="{Binding LastThirtyDaysControlledEnvironmentDayCount,Mode=TwoWay}" 
                                                                           ValueTemplate="{StaticResource DaysTextTemplate}" 
                                                                           ReadOnlyValueTemplate="{StaticResource ReadOnlyDaysTextTemplate}"/>
                                            <CustomControls:AddRemoveCommentControl Text="{Binding LastThirtyDaysControlledEnvironmentDayCountNote, Mode=TwoWay}"/>

