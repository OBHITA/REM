                    <CustomControls:Editor MaximizeHeader="DENS Interview" Style="{StaticResource TileEditorHeaderStyle}" StartOpen="False"  SaveCommand="{Binding SaveCommand}" CancelCommand="{Binding CancelCommand}" AccessControlManager="{Binding AccessControlManager}" CanMaximize="True">

                        <CustomControls:Editor.Expanders>
