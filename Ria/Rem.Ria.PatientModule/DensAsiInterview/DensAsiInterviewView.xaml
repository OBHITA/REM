                                            </CustomControls:EditableStackPanel.Resources>
                                            <CustomControls:EditableStackPanel Margin="0 0 0 50">
                                                <CustomControls:EditableStackPanel.Resources>
                                                    <Style TargetType="CustomControls:NonResponseQuestionControl" BasedOn="{StaticResource DensAsiNonResponseQuestionStyle}"/>
                                                    <Style TargetType="CustomControls:BasicScreenQuestionControl" BasedOn="{StaticResource DensAsiBasicScreenQuestionStyle}"/>
                                                </CustomControls:EditableStackPanel.Resources>
