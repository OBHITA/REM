                                                    <TextBlock Text="{Binding AdmissionDate, StringFormat='{}{0:MM/dd/yyyy}', FallbackValue='-', TargetNullValue='-'}" Grid.Column="1" HorizontalAlignment="Left" Margin="68,0,0,0"/>
                                                </CustomControls:EditableGrid>
                                               
                                                <CustomControls:BasicScreenQuestionControl Question="G5. Date of Interview:"
                                                                           Hint="These dates often differ. If you do not know when the person will be admitted, enter XX/XX/XXXX. If date of admission and date of interview are same date fill in both with same date. Clicking on the small arrow will produce a pop-up calendar. "
                                                                           Value="{Binding InterviewDate,Mode=TwoWay}" Margin="0"
                                                                           ValueTemplate="{StaticResource DateTemplate}" ReadOnlyValueTemplate="{StaticResource ReadOnlyBasicDateTemplate}"/>
                                                <CustomControls:AddRemoveCommentControl Text="{Binding InterviewDateNote, Mode=TwoWay}"/>
                                                
                                                <CustomControls:BasicScreenQuestionControl Question="G8. Class:"  
                                                                           Hint="Most ASIs for the DENS study will becoded &quot;intake&quot;. ASI's done on or near admission are &quot;intakes&quot; even if the person has been in your treatment program before.Follow-up ASIs are generally completed byinterviewers completing follow-up studies."
