			</CustomControls:Editor.HeaderTemplate>
			<CustomControls:Editor.Expanders>
				<CustomControls:EditableExpander x:Name="Profile" Header="Profile" Content="{Binding EditingDto.PatientProfile}">
					<CustomControls:EditableExpander.ContentTemplate>
						<DataTemplate>
							<Border Style="{StaticResource EditFormSectionBorderStyle}">
								<StackPanel Orientation="Vertical">
									<StackPanel Orientation="Horizontal">
										<StackPanel Style="{StaticResource EditFormStackPanelStyle}">
											<TextBlock Text="First Name" Style="{StaticResource EditFormLabelTextBlockStyle}"/>
											<TextBlock x:Name="FirstName" Text="{Binding FirstName, FallbackValue='-', TargetNullValue='-'}"  Style="{StaticResource EditFormDataElementTextBlockStyle}"/>
										</StackPanel>
										<StackPanel Style="{StaticResource EditFormStackPanelStyle}">
											<TextBlock Text="Middle Name" Style="{StaticResource EditFormLabelTextBlockStyle}"/>
											<TextBlock x:Name="MiddleName" Text="{Binding MiddleName, FallbackValue='-', TargetNullValue='-'}" Style="{StaticResource EditFormDataElementTextBlockStyle}"/>
										</StackPanel>
										<StackPanel Style="{StaticResource EditFormStackPanelStyle}">
											<TextBlock Text="Last Name" Style="{StaticResource EditFormLabelTextBlockStyle}"/>
											<TextBlock x:Name="LastName" Text="{Binding LastName, FallbackValue='-', TargetNullValue='-'}" Style="{StaticResource EditFormDataElementTextBlockStyle}"/>
										</StackPanel>
										<StackPanel Style="{StaticResource EditFormStackPanelStyle}">
											<TextBlock Text="Suffix Name" Style="{StaticResource EditFormLabelTextBlockStyle}"/>
											<TextBlock x:Name="Suffix" Text="{Binding SuffixName, StringFormat='({0})', FallbackValue='-', TargetNullValue='-'}"  Style="{StaticResource EditFormDataElementTextBlockStyle}"/>
										</StackPanel>
									</StackPanel>
									<StackPanel Orientation="Horizontal">
										<StackPanel Style="{StaticResource EditFormStackPanelStyle}">
											<TextBlock Text="Gender" Style="{StaticResource EditFormLabelTextBlockStyle}"/>
											<TextBlock x:Name="Gender" Text="{Binding PatientGender, FallbackValue='-', TargetNullValue='-'}" Style="{StaticResource EditFormDataElementTextBlockStyle}"/>
										</StackPanel>
										<StackPanel Style="{StaticResource EditFormStackPanelStyle}">
											<TextBlock Text="Date Of Birth" Style="{StaticResource EditFormLabelTextBlockStyle}"/>
											<TextBlock x:Name="BirthDate" Text="{Binding BirthDate,StringFormat='{}{0:MM/dd/yyyy}', FallbackValue='-', TargetNullValue='-'}" Style="{StaticResource EditFormDataElementTextBlockStyle}"/>
										</StackPanel>
										<StackPanel Style="{StaticResource EditFormStackPanelStyle}">
											<TextBlock Text="Date Of Death" Style="{StaticResource EditFormLabelTextBlockStyle}"/>
											<TextBlock x:Name="DeathDate" Text="{Binding DeathDate,StringFormat='{}{0:MM/dd/yyyy}', FallbackValue='-', TargetNullValue='-'}" Style="{StaticResource EditFormDataElementTextBlockStyle}"/>
										</StackPanel>
									</StackPanel>
									<Border Style="{StaticResource HorizontalLineEditFormDivider}"/>
									<StackPanel Orientation="Horizontal">
										<StackPanel Style="{StaticResource EditFormStackPanelStyle}">
											<TextBlock Text="ContactPreference"  Style="{StaticResource EditFormLabelTextBlockStyle}"/>
											<TextBlock x:Name="ContactPreference" Text="{Binding ContactPreference, FallbackValue='-', TargetNullValue='-'}" Style="{StaticResource EditFormDataElementTextBlockStyle}"/>
										</StackPanel>
										<StackPanel Style="{StaticResource EditFormStackPanelStyleL}">
											<TextBlock Text="Email Address" Style="{StaticResource EditFormLabelTextBlockStyle}"/>
											<TextBlock x:Name="EmailAddress" Text="{Binding EmailAddress,StringFormat='{}{0:MM/dd/yyyy}', FallbackValue='-', TargetNullValue='-'}" Style="{StaticResource EditFormDataElementTextBlockStyle}"/>
										</StackPanel>
									</StackPanel>
								</StackPanel>
							</Border>
						</DataTemplate>
					</CustomControls:EditableExpander.ContentTemplate>
					<CustomControls:EditableExpander.ContentEditTemplate>
						<DataTemplate>
							<Border Style="{StaticResource EditFormSectionBorderStyle}">
								<StackPanel Orientation="Vertical">
									<StackPanel Orientation="Horizontal">
										<StackPanel Style="{StaticResource EditFormStackPanelStyle}">
											<TextBlock Text="First Name"  Style="{StaticResource EditFormLabelTextBlockStyle}"/>
											<telerik:RadMaskedTextBox x:Name="FirstName" EmptyContent="First Name" MaskedText="{Binding FirstName,Mode=TwoWay}" MaskType="None" EmptyContentTemplate="{StaticResource WatermarkTemplate}"/>
										</StackPanel>
										<StackPanel Style="{StaticResource EditFormStackPanelStyle}">
											<TextBlock Text="Middle Name"  Style="{StaticResource EditFormLabelTextBlockStyle}"/>
											<telerik:RadMaskedTextBox x:Name="MiddleName" EmptyContent="Middle Name" MaskedText="{Binding MiddleName,Mode=TwoWay}" MaskType="None" EmptyContentTemplate="{StaticResource WatermarkTemplate}"/>
										</StackPanel>
										<StackPanel Style="{StaticResource EditFormStackPanelStyle}">
											<TextBlock Text="Last Name" Style="{StaticResource EditFormLabelTextBlockStyle}"/>
											<telerik:RadMaskedTextBox x:Name="LastName" EmptyContent="Last Name" MaskedText="{Binding LastName,Mode=TwoWay}" MaskType="None" EmptyContentTemplate="{StaticResource WatermarkTemplate}"/>
										</StackPanel>
										<StackPanel Style="{StaticResource EditFormStackPanelStyle}">
											<TextBlock Text="Suffix Name" Style="{StaticResource EditFormLabelTextBlockStyle}"/>
											<telerik:RadMaskedTextBox x:Name="Suffix" EmptyContent="Suffix Name" MaskedText="{Binding SuffixName,Mode=TwoWay}" MaskType="None" EmptyContentTemplate="{StaticResource WatermarkTemplate}"/>
										</StackPanel>
									</StackPanel>
									<StackPanel Orientation="Horizontal">
										<StackPanel Style="{StaticResource EditFormStackPanelStyle}">
											<TextBlock Text="Gender" Style="{StaticResource EditFormLabelTextBlockStyle}"/>
											<telerik:RadComboBox x:Name="Gender" ItemsSource="{Binding Source={StaticResource RootDataContext},Path=DataSource.LookupValueLists[PatientGender]}" DisplayMemberPath="Name" SelectedItem="{Binding PatientGender,Mode=TwoWay}"/>
										</StackPanel>
										<StackPanel Style="{StaticResource EditFormStackPanelStyle}">
											<TextBlock Text="Date Of Birth"  Style="{StaticResource EditFormLabelTextBlockStyle}"/>
											<telerik:RadDatePicker x:Name="BirthDate" SelectedValue="{Binding BirthDate,Mode=TwoWay}" DateTimeWatermarkContent="mm/dd/yyyy" />
										</StackPanel>
										<StackPanel Style="{StaticResource EditFormStackPanelStyle}">
											<TextBlock Text="Date Of Death" Style="{StaticResource EditFormLabelTextBlockStyle}"/>
											<telerik:RadDatePicker x:Name="EndDate" SelectedValue="{Binding DeathDate,Mode=TwoWay}" DateTimeWatermarkContent="mm/dd/yyyy"/>
										</StackPanel>
									</StackPanel>
									<Border Style="{StaticResource HorizontalLineEditFormDivider}"/>
									<StackPanel Orientation="Horizontal">
										<StackPanel Style="{StaticResource EditFormStackPanelStyle}">
											<TextBlock  Text="Contact Preference" Style="{StaticResource EditFormLabelTextBlockStyle}"/>
											<telerik:RadComboBox x:Name="ContactPreference" ItemsSource="{Binding Source={StaticResource RootDataContext},Path=DataSource.LookupValueLists[ContactPreference]}" DisplayMemberPath="Name" SelectedItem="{Binding ContactPreference,Mode=TwoWay}"/>
										</StackPanel>
										<StackPanel Style="{StaticResource EditFormStackPanelStyleM}">
											<TextBlock Text="Email Address" Style="{StaticResource EditFormLabelTextBlockStyle}"/>
											<telerik:RadMaskedTextBox x:Name="EmailAddress" EmptyContent="Email Address" MaskedText="{Binding EmailAddress,Mode=TwoWay}" MaskType="None" EmptyContentTemplate="{StaticResource WatermarkTemplate}"/>
										</StackPanel>
									</StackPanel>
								</StackPanel>
							</Border>
						</DataTemplate>
					</CustomControls:EditableExpander.ContentEditTemplate>
				</CustomControls:EditableExpander>
				<CustomControls:EditableExpander x:Name="AlternateNames" Header="Alternate Names" Content="{Binding EditingDto.PatientAliases}">
					<CustomControls:EditableExpander.Resources>
						<CollectionViewSource x:Key="PatientAliasesSource" Source="{Binding EditingDto.PatientAliases.Aliases}">
							<CollectionViewSource.SortDescriptions>
								<ComponentModel:SortDescription PropertyName="PatientAliasType.SortOrderNumber" Direction="Ascending"/>
								<ComponentModel:SortDescription PropertyName="PatientAliasType.Name" Direction="Ascending"/>
							</CollectionViewSource.SortDescriptions>
						</CollectionViewSource>
					</CustomControls:EditableExpander.Resources>
					<CustomControls:EditableExpander.ContentTemplate>
						<DataTemplate>
							<Border Style="{StaticResource EditFormSectionBorderStyle}">
								<StackPanel Orientation="Vertical">
									<StackPanel Orientation="Horizontal">
										<StackPanel Style="{StaticResource EditFormStackPanelStyle}">
											<TextBlock Text="Alternate Name Types" Style="{StaticResource EditFormLabelTextBlockStyle}"/>
										</StackPanel>
										<StackPanel Style="{StaticResource EditFormStackPanelStyle}">
											<TextBlock Text="First Name" Style="{StaticResource EditFormLabelTextBlockStyle}"/>
