										<DoubleAnimation Duration="0" To="0.091" Storyboard.TargetProperty="(Shape.Fill).(GradientBrush.GradientStops)[2].(GradientStop.Offset)" Storyboard.TargetName="FocusVisualElement" d:IsOptimized="True"/>
