										<DoubleAnimation Duration="0" To="0.839" Storyboard.TargetProperty="(Shape.Fill).(GradientBrush.GradientStops)[0].(GradientStop.Offset)" Storyboard.TargetName="FocusVisualElement" d:IsOptimized="True"/>
